<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffff"
    android:orientation="vertical"
    tools:context=".Activity2">

    <LinearLayout
        android:layout_marginTop="10dp"
        android:paddingBottom="10dp"
        android:background="@drawable/black"
        android:layout_gravity="center"
        android:layout_width="350dp"
        android:layout_height="60dp">

    <TextView
        android:layout_gravity="center"
        android:layout_marginStart="90dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:text="APPETIZER'S"
        android:textColor="#ffffff"
        android:textSize="30sp"
        android:textStyle="bold">

    </TextView>
</LinearLayout>

    <LinearLayout
        android:layout_marginStart="10dp"
        android:layout_width="370dp"
        android:layout_height="75dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/app1"
        android:paddingBottom="5dp">

        <Button
            android:textColor="@color/white"
            android:id="@+id/appetizer1"
            android:layout_width="100dp"
            android:layout_height="40dp"
            android:layout_marginStart="20dp"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="10dp"
            android:layout_marginEnd="20dp"
            android:background="@drawable/black"
            android:text="Recipe"
            android:textAllCaps="false"
            android:textSize="25sp">

        </Button>

        <TextView

            android:layout_width="190dp"
            android:layout_height="70dp"
            android:background="@drawable/brown"
            android:gravity="center"
            android:text="Spinach Artichoke\n Zucchinni Bites"
            android:textColor="#000000"
            android:textSize="18sp"
            android:textStyle="bold">

        </TextView>
    </LinearLayout>
    <LinearLayout
        android:layout_marginStart="10dp"
        android:layout_width="370dp"
        android:layout_height="75dp"
        android:layout_marginTop="25dp"
        android:background="@drawable/app2"
        android:paddingBottom="15dp">

        <Button
            android:textColor="@color/white"
            android:id="@+id/appetizer2"
            android:layout_width="100dp"
            android:layout_height="40dp"
            android:layout_marginStart="20dp"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="15dp"
            android:layout_marginEnd="20dp"
            android:background="@drawable/black"
            android:text="Recipe"
            android:textAllCaps="false"
            android:textSize="25sp">
        </Button>

        <TextView
            android:layout_marginTop="7dp"
            android:layout_width="190dp"
            android:layout_height="50dp"
            android:background="@drawable/brown"
            android:gravity="center"
            android:text="Cranberry Brie Bites"
            android:textColor="#000000"
            android:textSize="18sp"
            android:textStyle="bold">

        </TextView>
    </LinearLayout>
    <LinearLayout
        android:layout_marginStart="10dp"
        android:layout_width="370dp"
        android:layout_height="75dp"
        android:layout_marginTop="25dp"
        android:background="@drawable/app3"
        android:paddingBottom="15dp">

        <Button
            android:textColor="@color/white"
            android:id="@+id/appetizer3"
            android:layout_width="100dp"
            android:layout_height="40dp"
            android:layout_marginStart="20dp"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="15dp"
            android:layout_marginEnd="20dp"
            android:background="@drawable/black"
            android:text="Recipe"
            android:textAllCaps="false"
            android:textSize="25sp">

        </Button>

        <TextView
            android:layout_marginTop="7dp"
            android:layout_width="190dp"
            android:layout_height="50dp"
            android:background="@drawable/brown"
            android:gravity="center"
            android:text="Carrots In A Blanket"
            android:textColor="#000000"
            android:textSize="18sp"
            android:textStyle="bold">

        </TextView>
    </LinearLayout>
    <LinearLayout
        android:layout_marginStart="10dp"
        android:layout_width="370dp"
        android:layout_height="75dp"
        android:layout_marginTop="25dp"
        android:background="@drawable/app4"
        android:paddingBottom="5dp">

        <Button
            android:textColor="@color/white"
            android:id="@+id/appetizer4"
            android:layout_width="100dp"
            android:layout_height="40dp"
            android:layout_marginStart="20dp"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="15dp"
            android:layout_marginEnd="20dp"
            android:background="@drawable/black"
            android:text="Recipe"
            android:textAllCaps="false"
            android:textSize="25sp">

        </Button>

        <TextView

            android:layout_width="190dp"
            android:layout_height="60dp"
            android:background="@drawable/brown"
            android:gravity="center"
            android:text="Ham n' Cheese Biscuit Stacks"
            android:textColor="#000000"
            android:textSize="18sp"
            android:textStyle="bold">

        </TextView>
    </LinearLayout>
    <LinearLayout
        android:layout_marginStart="10dp"
        android:layout_width="370dp"
        android:layout_height="75dp"
        android:layout_marginTop="25dp"
        android:background="@drawable/app5"
        android:paddingBottom="15dp">

        <Button
            android:textColor="@color/white"
            android:id="@+id/appetizer5"
            android:layout_width="100dp"
            android:layout_height="40dp"
            android:layout_marginStart="20dp"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="15dp"
            android:layout_marginEnd="20dp"
            android:background="@drawable/black"
            android:text="Recipe"
            android:textAllCaps="false"
            android:textSize="25sp">

        </Button>

        <TextView
            android:layout_marginTop="7dp"
            android:layout_width="190dp"
            android:layout_height="50dp"
            android:background="@drawable/brown"
            android:gravity="center"
            android:text="Tortilla Pinwheels"
            android:textColor="#000000"
            android:textSize="18sp"
            android:textStyle="bold">

        </TextView>
    </LinearLayout>
    <LinearLayout
        android:layout_marginBottom="10dp"
        android:layout_marginStart="10dp"
        android:layout_width="370dp"
        android:layout_height="75dp"
        android:layout_marginTop="25dp"
        android:background="@drawable/app6"
        android:paddingBottom="5dp">

        <Button
            android:textColor="@color/white"
            android:id="@+id/appetizer6"
            android:layout_width="100dp"
            android:layout_height="40dp"
            android:layout_marginStart="20dp"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="15dp"
            android:layout_marginEnd="20dp"
            android:background="@drawable/black"
            android:text="Recipe"
            android:textAllCaps="false"
            android:textSize="25sp">

        </Button>

        <TextView

            android:layout_width="190dp"
            android:layout_height="70dp"
            android:background="@drawable/brown"
            android:gravity="center"
            android:text="Bacon And Sun-Dried Tomato Phyllo Tarts"
            android:textColor="#000000"
            android:textSize="17sp"
            android:textStyle="bold">

        </TextView>
    </LinearLayout>

    <ImageView
        android:layout_width="360dp"
        android:layout_height="50dp"
        android:layout_marginTop="23dp"
        android:background="@drawable/browndark">

    </ImageView>
</LinearLayout>



